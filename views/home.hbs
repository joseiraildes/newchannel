<br><br><br>
<div class="container">
  <div class="row">
    {{#each post}}
      <div class="col-md-6">
        <div class="card">
          <div class="card-header d-flex justify-content-between">
            <span>
              <a class="text-muted" href="/@{{nome}}">@{{nome}}</a>
            </span>
          </div>
          <div class="card-body">
            <a href="/@{{nome}}/{{id}}/{{titulo}}">{{titulo}}</a>
            <br>
            <strong class="text-small">
              {{data}}
            </strong>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>